{
  "name": "vite-project",
  "engines": {
    "node": ">=17"
  },
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm-run-all test lint:all"
    }
  },
  "scripts": {
    "dev": "vite",
    "test": "mocha",
    "start": "vite build; node server.cjs",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "npx tsc --noEmit && npx eslint . --ext .ts && npx stylelint \"**/*.css\"",
    "lint:es": "eslint . --ext .ts",
    "lint:es:fix": "eslint . --ext .ts --fix",
    "lint:style": "stylelint \"**/*.css\"",
    "lint:style:fix": "stylelint \"**/*.css\" --fix"
  },
  "dependencies": {
    "express": "^4.21.2",
    "handlebars": "^4.7.8",
    "uuid": "^11.0.5"
  },
  "devDependencies": {
    "@types/chai": "^5.2.1",
    "@types/mocha": "^10.0.10",
    "@types/sinon": "^17.0.4",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "chai": "^5.1.2",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "mocha": "^11.1.0",
    "prettier": "^2.7.1",
    "sinon": "^19.0.2",
    "stylelint": "^14.13.0",
    "stylelint-config-standard": "^28.0.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "vite": "^6.1.0"
  }
}
